{
  "name": "express-js",
  "version": "1.0.0",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --watchAll",
    "dev": "nodemon --import=extensionless/register server.js",
    "start": "node --import=extensionless/register server.js",
    "prisma": "npx prisma db pull && npx prisma generate",
    "sequelize-auto": "npx sequelize-auto -h \"$DB_HOST\" -d \"$DB_NAME\" -u \"$DB_USER\" -x \"$DB_PASS\" -p \"$DB_PORT\" --dialect mysql -o ./src/models/sequelize-auto",
    "update-models": "npm run prisma && npm run sequelize-auto",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --check src/**/*.{ts,tsx,css}",
    "format:fix": "prettier --write \"src/**/*.{js,ts,jsx,tsx,json,css,md}\" --ignore-path .prettierignore"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@jest/types": "^30.0.5",
    "@prisma/client": "^6.15.0",
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.7.0",
    "cookie-parser": "^1.4.7",
    "express": "^5.1.0",
    "extensionless": "^1.9.9",
    "graphql": "^16.11.0",
    "graphql-http": "^1.22.4",
    "jest": "^30.0.5",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "mysql2": "^3.14.4",
    "nodemailer": "^7.0.5",
    "nodemon": "^3.1.10",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "prettier": "^3.6.2",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "eslint": "^9.33.0",
    "prisma": "^6.15.0",
    "sequelize": "^6.37.7",
    "sequelize-auto": "^0.8.8"
  }
}
